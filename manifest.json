{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "Sound Tab Control",
    "description": "Controle seus sons de videos e audios por ABA no CHROME!", 
    "author": "Lucas Yuji Yoshimine",
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "48": "48.png",
       "128": "128.png"
     },
     "background": {
        "scripts": ["background.js"],
        "persistent": false
     },
    "browser_action": {
        "default_title": "Sound Control",
        "default_popup": "index.html",
        "default_icon": "128.png"
    },
    "permissions": [
        "activeTab", 
        "tabs",
        "http://*/*", 
        "https://*/*",
        "storage"
    ]
}